{
  "name": "npm-version",
  "displayName": "NPM Version",
  "description": "show npm version directly",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "assets/icon.png",
  "contributes": {
    "commands": [
      {
        "command": "npm-version.open",
        "title": "Open In Npm"
      }
    ]
  },
  "scripts": {
    "vsce": "vsce package",
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/absolute": "^0.0.31",
    "@types/find-up": "^4.0.0",
    "@types/lodash-es": "^4.17.6",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.6",
    "@types/open": "^6.2.1",
    "@types/pify": "^5.0.4",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8",
    "eslint": "^8.56.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "absolute": "^0.0.1",
    "find-up": "^7.0.0",
    "lodash-es": "^4.17.21",
    "open": "^8.0.0",
    "pify": "^3.0.0"
  }
}
